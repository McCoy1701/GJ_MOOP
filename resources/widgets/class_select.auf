[WT_CONTAINER.class_select]
(x,y):(
    calc(SCREEN.w / 2 - THIS.w / 2),
    calc(THIS.h * 3)
)
boxed:1
padding:6
align:1
justify:1
flex:1
spacing:50
click_sound:"resources/soundeffects/menu_click.wav"
hover_sound:"resources/soundeffects/menu_move.wav"
fg:[255,255,0,255]
bg:[0,0,0,200]
[[WT_BUTTON.mercenary]]
(x,y):(10,10)
label:"Mercenary"
toggle_label:1
boxed:1
hidden:0
padding-x:38
padding-y: 8
texture:1
(w,h):(20,20)
background:"resources/assets/UI/button/Red_Button.png"
pressed:"resources/assets/UI/button/Red_Button_p.png"
hovering:"resources/assets/UI/button/Red_Button_h.png"
disabled:"resources/assets/UI/button/Red_Button_d.png"
(text_x,text_y):(3,3)
button_drop_offset:5
fg:[0,0,0,255]
bg:[164,221,219,0]
