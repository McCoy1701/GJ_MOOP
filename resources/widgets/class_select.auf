[WT_CONTAINER.title_panel]
(x,y):(
    calc(class_select.x + THIS.w / 2),
    calc( THIS.h / 2 )
)
label:""
toggle_label:0
boxed:1
padding:5
justify:1
align:1
flex:2
spacing:0
texture:0
(w,h):(
    calc(class_select.w / 2),
    calc(class_select.h)
)
bg:[0,0,0,255]
[[WT_OUTPUT.title_text]]
label:"Choose Your Class"
boxed:1
hidden:0
visible_length:32
text_offset:0
fg:[255,255,255,255]
bg:[title_panel.bg]
